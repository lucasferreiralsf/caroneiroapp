type User {
  id: ID! @id
  firstName: String!
  lastName: String!
  primaryPhoneNumber: String @unique
  secondaryPhoneNumber: String
  email: String! @unique
  password: String
  emailIsVerified: Boolean @default(value: false)
  primaryPhoneNumberIsVerified: Boolean @default(value: false)
  googleId: String @unique
  facebookId: String @unique
  ownerTravels: [Travel!]! @relation(name: "OwnerTravels", link: INLINE)
  travelsAsPassenger: [Travel!]! @relation(name: "TravelsAsPassenger", link: INLINE)
  emailToken: String
  createdAt: DateTime! @createdAt
  updatedAt: DateTime! @updatedAt
}